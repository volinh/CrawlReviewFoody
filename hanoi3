<script>
    $(window).load(function () {
        var hash = window.location.hash;
        if (hash) {
            if (hash.indexOf("delivery") !== -1)
                setTimeout(function () {
                    $('html, body').animate({
                        scrollTop: $("#box-delivery").offset().top - 60
                    }, 500);
                }, 500);

            else if (hash.indexOf("booking") !== -1)
                setTimeout(function () {
                    $('html, body').animate({
                        scrollTop: $("#box-tablenow").offset().top - 60
                    }, 500);
                }, 500);
        }
    });
</script>

    


